<div class="top-spc"></div>
<section>
<div class="container">
                <div class="row">
<h2>Log inoo</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name),html:{class:"form-horizontal"}) do |f| %>

   <div class="form-group">
    <label class="control-label col-sm-2" for="email">Email:</label>
    <div class="col-sm-10">
    <%= f.text_field :login, class: 'form-control', placeholder: 'Username' %>
      
    </div>
  </div>
  <div class="form-group">
    <label class="control-label col-sm-2" for="pwd">Password:</label>
    <div class="col-sm-10">
    <%= f.password_field :password, autocomplete: "off",class: 'form-control', placeholder: 'Enter Password' %>
      
    </div>
  </div>
  <% if devise_mapping.rememberable? -%>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <div class="checkbox">
        <label><%= f.check_box :remember_me %> Remember me</label>
      </div>
    </div>
  </div>
  <% end -%>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
    <%= f.submit "Log in", class:'btn btn-default' %>
      </div>
  </div>

  
<% end %>

<%= render "users/shared/links" %>
</div></div></section>